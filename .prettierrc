{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "importOrder": [
    "^api/(.*)$",
    "^contracts/(.*)$",
    "^features/(.*)$",
    "^models/(.*)$",
    "^pages/(.*)$",
    "^typings/(.*)$",
    "^shared/lib(.*)$",
    "^shared/ui(.*)$",
    "^shared/enums(.*)$",
    "^shared/stubs(.*)$",
    "^shared/typings(.*)$",
    "^assets/(.*)$","^[./]"],
  "importOrderSeparation": true
}